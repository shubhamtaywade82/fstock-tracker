<table class="table table-bordered table-hover col-sm-9 mt-4">
     <thead>
        <tr>
        <th scope="col">Ticker</th>
        <th scope="col">Name</th>
        <th scope="col">Price</th>
        <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        <% @tracked_stocks.each do |stock| %>
            <tr>
            <th scope="row"><%= stock.ticker %></th>
            <td><%= stock.name %> </td>
            <td><%= Stock.new_lookup(stock.ticker).last_price %> </td>
            

            
            <td><%= link_to 'Remove', user_stock_path(stock) , method: :delete, data: {confirm: 'Are you sure'}, class: 'btn col-sm btn-sm btn-warning' %></td>
                
            </tr>
        <% end %>
    </tbody>
</table>